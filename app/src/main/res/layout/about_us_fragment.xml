<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.about_us.AboutUsFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="5dp">

        <TextView
            android:id="@+id/about_us_txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="App developed by : Junaid Hassan"
            android:textSize="18sp"
            android:textStyle="bold" />
    <TextView
        android:id="@+id/contact_about_us_txt2"
        android:textColor="#03A9F4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/string_about_us_1"
        android:textSize="18sp"
        android:textStyle="bold"/>
        <TextView
            android:id="@+id/email_about_us_txt3"
            android:textColor="#03A9F4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_about_us_2"
            android:textSize="18sp"
            android:textStyle="bold"/>
    </LinearLayout>

</FrameLayout>