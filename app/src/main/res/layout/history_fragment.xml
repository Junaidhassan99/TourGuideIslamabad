<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=""
    android:layout_margin="5dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/history_layout_txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_1"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/history_layout_txt2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_2" />

        <ImageView
            android:id="@+id/history_layout_img9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/img9" />

        <TextView
            android:id="@+id/history_layout_txt3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_3"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/history_layout_txt4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_4" />

        <ImageView
            android:id="@+id/history_layout_img10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/img10" />

        <TextView
            android:id="@+id/history_layout_txt5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_5"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/history_layout_txt6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/string_history_6" />

        <ImageView
            android:id="@+id/history_layout_img11"
            android:layout_width="match_parent"
            android:layout_height="251dp"
            android:src="@drawable/img11" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>